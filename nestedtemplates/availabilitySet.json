{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "resourceNames": {
      "type": "object"
    },
    "location": {
      "type": "string"
    }
  },
  "variables": {},
  "resources": [
    {
      "type": "Microsoft.Compute/availabilitySets",
      "name": "[parameters('resourceNames').availabilitySets.ad]",
      "apiVersion": "2015-06-15",
      "tags": {
        "Project": "BluePrint"
      },
      "location": "[parameters('location')]"
    },
    {
      "type": "Microsoft.Compute/availabilitySets",
      "name": "[parameters('resourceNames').availabilitySets.sql]",
      "apiVersion": "2015-06-15",
      "tags": {
        "Project": "BluePrint"
      },
      "location": "[parameters('location')]",
      "properties": {
        "platformFaultDomainCount": "3",
        "platformUpdateDomainCount": "3"
      }
    },
    {
      "type": "Microsoft.Compute/availabilitySets",
      "name": "[parameters('resourceNames').availabilitySets.web]",
      "apiVersion": "2015-06-15",
      "tags": {
        "Project": "BluePrint"
      },
      "location": "[parameters('location')]",
      "properties": {
        "platformFaultDomainCount": "2",
        "platformUpdateDomainCount": "2"
      }
    }
  ],
  "outputs": {}
}